#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules/openapi-format/bin/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules/openapi-format/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules/openapi-format/bin/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules/openapi-format/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/openapi-format@1.25.0/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
