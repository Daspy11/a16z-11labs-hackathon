#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/src/cli-validator/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/src/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/src/cli-validator/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/src/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules/ibm-openapi-validator/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/ibm-openapi-validator@1.33.2/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli-validator/index.js" "$@"
else
  exec node  "$basedir/../../src/cli-validator/index.js" "$@"
fi
