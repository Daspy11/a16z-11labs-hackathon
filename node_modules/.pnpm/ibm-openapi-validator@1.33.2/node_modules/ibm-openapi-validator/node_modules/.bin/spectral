#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/dist/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/dist/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/@stoplight+spectral-cli@6.14.2/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/dist/index.js" "$@"
else
  exec node  "$basedir/../../../../../@stoplight+spectral-cli@6.14.2/node_modules/@stoplight/spectral-cli/dist/index.js" "$@"
fi
