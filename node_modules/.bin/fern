#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/fern-api@0.53.15/node_modules/fern-api/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/fern-api@0.53.15/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/fern-api@0.53.15/node_modules/fern-api/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/fern-api@0.53.15/node_modules:/Users/stuartjohnson/Code/Hackathons/2025-a16z-11l/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fern-api/cli.cjs" "$@"
else
  exec node  "$basedir/../fern-api/cli.cjs" "$@"
fi
